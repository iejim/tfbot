{
    "env": {
        "linux-headers": "${workspaceFolder}/../linux",
        "fw_packages": "${workspaceFolder}/robot/firmware/*/src/*/include",
        "tfbot_msgs" : "${workspaceFolder}/robot/teleop/devel/include"
    },
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",

                "${workspaceFolder}/robot/firmware/include_fw",
                "${workspaceFolder}/include/melodic/include",
                "${fw_packages}",
                "${tfbot_msgs}",
                "${linux-headers}/usr/include",
                "${linux-headers}/usr/include/c++/**",
                "${linux-headers}/libc6-dev-armhf-cross/arm-linux-gnueabihf/include",
                "${linux-headers}/linux-libc-dev-armhf-cross/arm-linux-gnueabihf/include"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX10.14.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang++"
        }
    ],
    "version": 4,
    
    "enableConfigurationSquiggles": true
}