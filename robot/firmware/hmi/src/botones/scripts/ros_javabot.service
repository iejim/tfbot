[Unit]
Description=ROS Programable JavaBot 
Requires=systemd-modules-load.service
Requires=wd_botones.service
BindsTo=wd_botones.service
Conflicts=ros_teleop.service

[Service]
User=ubuntu
PIDFile=/run/tfbot/ros_javabot.pid
#ExecStartPre=/usr/bin/rc_startup_routine
ExecStart=-/opt/tfbot/scripts/runner_javabot.sh
#ExecStop=/usr/bin/rc_kill

[Install]
WantedBy=wd_botones.service
